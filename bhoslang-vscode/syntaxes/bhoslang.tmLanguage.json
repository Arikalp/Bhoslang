{
  "scopeName": "source.bhoslang",
  "name": "Bhoslang",
  "patterns": [
    {
      "name": "keyword.control.bhoslang",
      "match": "\\b(bsdk|badalbsdk|likhbsdk|batabsdk)\\b"
    },
    {
      "name": "constant.numeric.bhoslang",
      "match": "\\b\\d+\\b"
    },
    {
      "name": "keyword.operator.bhoslang",
      "match": "[+\\-*/=]"
    },
    {
      "name": "variable.other.bhoslang",
      "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
    }
  ],
  "scopeName": "source.bhoslang"
}
